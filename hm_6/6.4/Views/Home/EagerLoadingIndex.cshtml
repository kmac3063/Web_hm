@{
    ViewData["Title"] = "Games";
}
<a href="EagerLoadingIndex">1</a>
<a href="ExplicitLoadingIndex">2</a>
<a href="LazyLoadingIndex">3</a>
<h3>Eager loading</h3>
<h3>Games</h3>
<table class="table-condensed table-info" cellpadding="15" cellspacing="15">
    <tr>
        <td>Game</td>
        <td>Release</td>
        <td>Category</td>
    </tr>

    @foreach (var game in Model)
    {
        <tr>
            <td>@game.Name</td>
            <td>@game.Release</td>
            <td>@game.Category.Name</td>
        </tr>
    }
</table>